<div class="form-header"><%= app.$T('header_unblock') %></div>
<form class="unblock form" method="post" action="#/unblock">
  <label><%= app.$T('label_puk') %></label>
  <input name="puk" type="password" class="span3" autofocus>
  <cite class="help-block"><i class="icon-info-sign"></i> &nbsp;<%= app.$T('invalid_puk_length').Format(@minLength, @maxLength) %></cite>
  <button type="submit" class="btn default"><i class="icon-unlock"></i> &nbsp;<%= app.$T('action_unblock') %></button>
</form>