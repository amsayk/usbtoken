<% sysmenu = require('lib/sysmenu') %>
<div class="toolsbar-inner">
	
	<div class="for-key">
		<div class="back btn-group">
		  <a title="<%= app.$T('back') %>" class="btn btn-large default btn-success btn-primary" href="#/keys"><i class="icon-arrow-left"></i></a>
		</div>
		<div class="actions btn-group">			
			<a title="<%= app.$T('tooltip_del') %>" href="#/key/<%= @id %>/purge" class="purge btn"><i class="icon-trash"></i></a>
			<a title="<%= app.$T('tooltip_download') %>" href="#/key/<%= @id %>/export" class="export btn"><i class="icon-download-alt"></i></a>
		</div>
		<%- sysmenu.render() %>
	</div>

</div>